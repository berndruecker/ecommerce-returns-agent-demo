    <bpmn:serviceTask id="Task_Salesforce_LookupContact" name="Lookup contact from Salesforce" zeebe:modelerTemplate="io.camunda.connectors.Salesforce.v1" zeebe:modelerTemplateVersion="3" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="io.camunda:http-json:1" />
        <zeebe:ioMapping>
          <zeebe:input source="= if supportCase.communicationContext.mobilePhone != null and supportCase.communicationContext.mobilePhone != &#34;&#34; then supportCase.communicationContext.mobilePhone else fromAi(toolCall.mobilePhone, &#34;Customer mobile phone (E.164)&#34;)" target="mobilePhone" />
          <zeebe:input source="= if supportCase.communicationContext.emailAddress != null and supportCase.communicationContext.emailAddress != &#34;&#34; then supportCase.communicationContext.emailAddress else fromAi(toolCall.emailAddress, &#34;Customer email address&#34;)" target="emailAddress" />
          <zeebe:input source="{{secrets.SALESFORCE_BASE_URL}}" target="baseUrl" />
          <zeebe:input source="v58.0" target="apiVersion" />
          <zeebe:input source="oauth-client-credentials-flow" target="authentication.type" />
          <zeebe:input source="soqlQuery" target="salesforceInteractionType" />
          <zeebe:input source="get" target="method" />
          <zeebe:input source="=&#34;SELECT Id, AccountId, Name, Email, MobilePhone &#34; +&#10;&#34;FROM Contact WHERE &#34; +&#10;&#34;(MobilePhone = &#39;&#34; + mobilePhone + &#34;&#39; AND MobilePhone != NULL)&#34;  +&#10;&#34;LIMIT 1&#34;" target="soqlQuery" />
          <zeebe:input source="={&#10;  q: soqlQuery&#10;}" target="queryParameters" />
          <zeebe:input source="=baseUrl + &#34;/services/data/&#34; + apiVersion + &#34;/query&#34;" target="url" />
          <zeebe:input source="=baseUrl + &#34;/services/oauth2/token&#34;" target="authentication.oauthTokenEndpoint" />
          <zeebe:input source="{{secrets.SALESFORCE_CLIENT_ID}}" target="authentication.clientId" />
          <zeebe:input source="{{secrets.SALESFORCE_CLIENT_SECRET}}" target="authentication.clientSecret" />
          <zeebe:input source="credentialsBody" target="authentication.clientAuthentication" />
          <zeebe:input source="20" target="connectionTimeoutInSeconds" />
        </zeebe:ioMapping>
        <zeebe:taskHeaders>
          <zeebe:header key="resultVariable" value="rawSalesforceCustomerResponse" />
          <zeebe:header key="resultExpression" value="=if count(response.body.records) &#62; 0 then&#10;  {&#10;    customer: {&#10;      id: response.body.records[1].Id,&#10;      name: response.body.records[1].Name,&#10;      email: response.body.records[1].Email,&#10;      mobilePhone: response.body.records[1].MobilePhone&#10;    },&#10;    toolCallResult: {&#10;      status: &#34;found&#34;,&#10;      customer: {&#10;        id: response.body.records[1].Id,&#10;        name: response.body.records[1].Name,&#10;        email: response.body.records[1].Email,&#10;        mobilePhone: response.body.records[1].MobilePhone&#10;      }&#10;    }&#10;  }&#10;  else&#10;  {&#10;    customer: null,&#10;    toolCallResult: {&#10;      status: &#34;not_found&#34;,&#10;      message: &#34;No matching contact found for provided identifiers.&#34;&#10;    }&#10;  }" />
          <zeebe:header key="elementTemplateVersion" value="3" />
          <zeebe:header key="elementTemplateId" value="io.camunda.connectors.Salesforce.v1" />
        </zeebe:taskHeaders>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0bgr4oc</bpmn:incoming>
      <bpmn:outgoing>Flow_0w9fba4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_0bgr4oc" sourceRef="Event_0aolqb7" targetRef="Task_Salesforce_LookupContact" />
    <bpmn:startEvent id="Event_0tu0evr" name="For testing">
      <bpmn:extensionElements>
        <zeebe:ioMapping>
          <zeebe:output source="={&#34;subject&#34;:&#34;WhatsApp Request&#34;,&#34;request&#34;:&#34;I want to return a router&#34;,&#34;originalMessage&#34;:{&#34;profileName&#34;:&#34;Bernd&#34;,&#34;text&#34;:&#34;I want to return a router&#34;,&#34;mobilePhone&#34;:&#34;+491711473461&#34;,&#34;senderId&#34;:&#34;491711473461&#34;,&#34;id&#34;:&#34;SM3dca9d2ec98b6b2b8b98b72cb096bbc2&#34;,&#34;from&#34;:&#34;whatsapp:+491711473461&#34;},&#34;communicationContext&#34;:{&#34;channel&#34;:&#34;whatsApp&#34;,&#34;from&#34;:&#34;whatsapp:+491711473461&#34;,&#34;lastMessageId&#34;:&#34;SM3dca9d2ec98b6b2b8b98b72cb096bbc2&#34;,&#34;mobilePhone&#34;:&#34;+491711473461&#34;}}" target="supportCase" />
          <zeebe:output source="={&#34;id&#34;:&#34;0039Q00001VcSaVQAV&#34;,&#34;name&#34;:&#34;Bernd Ruecker&#34;,&#34;email&#34;:&#34;bernd.it.depends.ruecker@gmail.com&#34;,&#34;mobilePhone&#34;:&#34;+491711473461&#34;}" target="customer" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_0ht0jj7</bpmn:outgoing>
